<view:UserControlView x:Class="Browser.Page.Wpf.Presenters.Container.PageContainerView"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:local="clr-namespace:Browser.Page.Wpf.Presenters.Container"
                 xmlns:view="clr-namespace:PresenterBase.View;assembly=PresenterBase"
                 mc:Ignorable="d"
                 d:DesignHeight="300" d:DesignWidth="300"
                 d:DataContext="{d:DesignInstance local:PageContainerViewModel}">
    <Grid>
        <ContentControl  Content="{Binding Path= WebContent}" />
    </Grid>
</view:UserControlView>
